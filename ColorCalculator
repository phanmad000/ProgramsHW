<script>
  function callOnOff(){
    if "oo-color" = #FFFFFF;
      document.getElementById('oo-color').setAttribute('fill', '#D3D3D3');
      callColors()
    if "oo-color" = #D3D3D3;
      document.getElementById('oo-color').setAttribute('fill', 'FFFFFF');
  
  function callColors(){
    function callRed(){
      document.getElementById('oo-color').setAttribute('fill', 'D32027');
    function callOrange(){
      document.getElementById('oo-color').setAttribute('fill', 'FBA919');
    function callYellow(){
      document.getElementById('oo-color').setAttribute('fill', 'DEE21E');
    function callGreen(){  
      document.getElementById('oo-color').setAttribute('fill', 'FFFFFF');
</script>

